body {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 14px;
    line-height: 1.42857143;
    color: #333;
    background-color: #fff;
}

div {
    display: block;
}
* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

pre {
    display: block;
    font-family: monospace;
    white-space: pre;
    margin: 1em 0px;
}
/* Custom Styles */
ul.nav-tabsT{
    width: 260px;
    height: 480px;
    margin-top:20px;
    border-radius: 4px;
    border: 1px solid #ddd;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.067);
}
ul.nav-tabsT li{
    margin-top: 10px;
    border-top: 1px solid #ddd;
    border: 0px;
}
ul.nav-tabsT li:first-child{
    border-top: none;
}
ul.nav-tabsT li a{
    margin: 0;
    padding: 8px 16px;
    border-radius: 0;
}
ul.nav-tabsT li.active a, ul.nav-tabs li.active a:hover{
    color: #fff;
    background: #0088cc;
    border: 1px solid #0088cc;
}
ul.nav-tabsT li:first-child a{
    border-radius: 4px 4px 0 0;
}
ul.nav-tabsT li:last-child a{
    border-radius: 0 0 4px 4px;
}
ul.nav-tabsT.affix{
    top: 20px; /* Set the top position of pinned element */
}
.icon18:visited {
    font-size: 0px;
}
.icon18:link {
    font-size: 0;
}
a:visited {
    color: #7e817e;
    text-decoration: none;
    font-family: Arial, Helvetica, sans-serif,'����',tahoma, Srial, helvetica, sans-serif;
}
a:link {
    color: #666;
