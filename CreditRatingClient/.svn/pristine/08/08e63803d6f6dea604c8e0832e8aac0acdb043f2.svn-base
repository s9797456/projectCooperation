    font-family: Arial, Helvetica, sans-serif,'����',tahoma, Srial, helvetica, sans-serif;
}
.main-container {
    max-width: 1500px;
    margin: 0 auto;
}
.main-container .block-title {
    font-size: 30px;
    text-align: center;
}
.center {
    text-align: center;
}
.step-block {
    padding-bottom: 60px;
}
.step-block .step {
    padding-top: 35px;
}

.step-block .step .singleblock {
    text-align: center;
    display: inline-block;
    font-size: 16px;
    vertical-align: top;
}

.step-block .step .singleblock .step-image {
    height: 80px;
    width: 160px;
    margin: auto;
    text-align: center;
}
.step-block .step .singleblock .step-image img {
    height: 74px
}
.step-block .step .singleblock .step-title {
    font-size: 16px;
    color: #565a5c;
}
.step-block .step .singleblock .step-num {
    position: relative;
    font-size: 24px;
    color: #577088;
}
.step-block .step .singleblock .step-num .right-line {
    position: absolute;
    height: 50px;
    width: 45%;
    right: 0px;
    top: 4px;
}
.step-block .step .singleblock .step-num .left-line {
    position: absolute;
    height: 50px;
    width: 45%;
    left: 0px;
    top: 4px;
}
.step-block .step .singleblock .step-detail {
    width: 225px;
    margin: 0 auto;
    text-align: justify !important;
    color: #999999;
    font-size: 14px;
    text-align: left;
    line-height: 27px;
}
hr {
    border-top: 6px solid #eef;
}
.corp-block {
    background-color: #f0f2f5;
    padding-top: 60px;
}
.black-bg {
    background-color: #2E3138;
}
.footer-text ul .break-icon {
    position: absolute;
    width: 30px;
    height: 20px;
    background-color: #2E3137;
    bottom: -11px;
}
.footer-logo {
    padding-top: 48px;
    padding-bottom: 30px;
}
.footer-logo img {
    width: 170px;
    height: 34px;
}
.footer-text ul {
    position: relative;
    list-style-type: none;
    margin: 0 auto;
    padding: 10px 0px 46px 0px;
    border-bottom: 1px solid #868686
}

.footer-text ul li a {
    text-decoration: none;
    color: #9aa5ad
}
.footer-text .footer-icp {
    padding: 35px;
    color: #868686;
    font-size: 14px;
}
@media (min-width: 768px){
    .step-block {
        min-width: 1200px;
        padding-top: 60px;
        height: 630px;
    }
    .step-block .step .singleblock {
        width: 19%;
    }
    .step-block .step .singleblock .step-title {
        margin: 10px 0;
    }

    .step-block .step .singleblock .step-num {
        padding-top: 10px;
        padding-bottom: 10px;
    }
    .black-bg {
        min-width: 1200px;
    }
    .footer-text {
        height: 200px
    }

    .footer-text ul {
        width: 580px
    }

    .footer-text ul li {
        padding: 0px 25px;
        display: inline
    }

    .footer-text ul li a {
        font-size: 16px
    }

    .footer-text ul .break-icon {
        left: 50%
    }
}

@media (max-width: 767px) {
    .footer-text ul {
        width: 100%;
        text-align: left
    }

    .footer-text ul li {
        padding-bottom: 20px;
        width: 50%;
        display: inline-block;
        text-align: center
    }

    .footer-text ul .break-icon {
        text-align: center;
        left: 47%
    }
}

