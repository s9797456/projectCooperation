 	margin-left: 30px;
}
.content .inside{
	/* background: url('../images/tab.png') no-repeat; */
	width:500px;
	height:400px;
	color:#fff;
	float:left;
	margin-top:50px;
}
.tab-contentImage{
	background: url('../images/tab.png') no-repeat ;
	width:500px;
	height:250px;
}
.tab-contentImage ul {
  margin-left: -40px;
  font-size: 14px;
}
.tab-contentImage ul li {
  color: #FFF;
  line-height: 26px;
  list-style-image: url(../images/dot_o.png);
  margin-left: 40px;
}
.tab-contentImage ul li span {
  color: #FFFFFF;
  float: right;
  margin-right: 50px;
}
.tab-contentImage a {text-decoration:none; }
.tab-contentImage a:link,.tab-contentImage a:visited{ color:#FFF;}
.tab-contentImage a:hover,.tab-contentImage a:active{ color:#ff8400;}

.newImage{
	background: url('../images/tabone.png') no-repeat -1px 0px;
	height:45px;
	width:200px;
}
.report{
	background: url('../images/tab.png') no-repeat ;
	margin-top:10px;
	width:500px;
	height:340px;
}
.report .reported{
	background: url('../images/column.png') no-repeat ;
	width:245px;
	height:281px;
	color:#fff;
	float:left;
	/* margin-left:30px; */
	margin-top:30px;
	/* border:1px solid #009966; */
}
.report .unreported{
	background: url('../images/column.png') no-repeat ;
	width:245px;
	height:281px;
	color:#fff;
	float:left;
	margin-left:10px;
	margin-top:30px;
