.resume-box {
	box-shadow: 10px 10px 20px #ddd;
	min-width:450px;
	
	border: 1px solid #e7e7e7;
	background: #fff;
	overflow: hidden;
	padding: 50px;
	margin: auto;
}

.browse-head {
	width: 100%;
	height: 30px;
	line-height: 30px;
	position: relative;
	overflow: hidden
}

.line-area {
	width: 100%;
	height: 0;
	line-height: 0;
	font-size: 0;
	border-top: 1px solid #e5e5e5;
	margin-top: 17px
}

.headLarea {
	position: absolute;
	left: 0;
	top: 0;
	height: 30px;
	background: #fff;
	padding-right: 20px
}

.headRarea {
	position: absolute;
	right: 0;
	top: 5px;
	height: 30px;
	background: #fff;
	padding-left: 20px;
	cursor: pointer
}


.resumeHeadtip em {
	display: inline-block;
	vertical-align: top;
	margin-top: 5px
}

.resume-edit {
	display: inline-block;
	height: 20px;
	line-height: 20px;
	color: #00a0e9;
	font-size: 14px
}

.resumeHeadtip {
	color: #ff6a0e;
	font-size: 14px
}


.resumeHeadtitle {
	height: 30px;
	line-height: 30px;
	background: #fff;
	color: #333;
	font-size: 20px;
	display: inline-block;
	margin-left: 5px
}

.browse-info {
	padding: 30px 0;
	overflow: hidden
}

.browse-info ul {
	float: left;
	width: 349px;
	padding-left: 50px
}


.browse-info ul li {
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	color: #666
}

