.text-overflow{
display:block;                     /*内联对象需加*/
width:auto;
word-break:keep-all;           /* 不换行 */
white-space:nowrap;          /* 不换行 */
overflow:hidden;               /* 内容超出宽度时隐藏超出部分的内容 */
text-overflow:ellipsis;         /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
}

.main-container {
    max-width: 1300px;
    margin: 0 auto;
}
.main-container .block-title {
    font-size: 30px;
    text-align: center;
}
.center {
    text-align: center;
}
.step-block {
    padding-bottom: 60px;
}
.step-block .step {
    padding-top: 35px;
}

.step-block .step .singleblock {
    text-align: center;
    display: inline-block;
    font-size: 16px;
    vertical-align: top;
}

.step-block .step .singleblock .step-image {
    height: 80px;
    width: 160px;
    margin: auto;
    text-align: center;
}
.step-block .step .singleblock .step-image img {
    height: 74px
}
.step-block .step .singleblock .step-title {
    font-size: 16px;
    color: #565a5c;
}
.step-block .step .singleblock .step-num {
    position: relative;
    font-size: 24px;
    color: #577088;
}
.step-block .step .singleblock .step-num .right-line {
    position: absolute;
    height: 50px;
    width: 45%;
    right: 0px;
    top: 4px;
}
.step-block .step .singleblock .step-num .left-line {
    position: absolute;
    height: 50px;
    width: 45%;
    left: 0px;
    top: 4px;
}
.step-block .step .singleblock .step-detail {
    width: 225px;
    margin: 0 auto;
    text-align: justify !important;
    color: #999999;
    font-size: 14px;
    text-align: left;
    line-height: 27px;
}