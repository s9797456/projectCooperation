    	/*缓存  动画*/
    	.modal-overlay {
             position: fixed;   /* 使用绝对定位或固定定位  */
             left: 0px;    
             top: 0px;
             width:100%;
             height:100%;
             text-align:center;
             z-index: 9999; /*脱流 层 */
             background-color:RGBA(195, 195, 195, 0.5); /* 遮罩层    背景色 + 透明度*/
        }
@keyframes line-scale-pulse-out {
  0% {
    -webkit-transform: scaley(1);
            transform: scaley(1); }

  50% {
    -webkit-transform: scaley(0.4);
            transform: scaley(0.4); }

  100% {
    -webkit-transform: scaley(1);
            transform: scaley(1); } }

.line-scale-pulse-out > div {
  background-color: #116893;
  width: 4px;
  height: 35px;
  border-radius: 2px;
  margin: 2px;
  -webkit-animation-fill-mode: both;
          animation-fill-mode: both;
  display: inline-block;
  -webkit-animation: line-scale-pulse-out 0.9s 0s infinite cubic-bezier(.85, .25, .37, .85);
          animation: line-scale-pulse-out 0.9s 0s infinite cubic-bezier(.85, .25, .37, .85); }
  .line-scale-pulse-out > div:nth-child(2), .line-scale-pulse-out > div:nth-child(4) {
    -webkit-animation-delay: 0.2s !important;
            animation-delay: 0.2s !important; }
  .line-scale-pulse-out > div:nth-child(1), .line-scale-pulse-out > div:nth-child(5) {
    -webkit-animation-delay: 0.4s !important;
            animation-delay: 0.4s !important; }
            
            
    	
/*placeholder占位文字样式*/
input[type="text"]::-webkit-input-placeholder {color:rgb(170, 170, 170); }
input[type="text"]::-moz-placeholder {color:rgb(170, 170, 170);} /* firefox 19+ */
input[type="text"]:-moz-placeholder {color:rgb(170, 170, 170);}
input[type="text"]:-ms-input-placeholder {color:rgb(170, 170, 170);} /* ie */ 