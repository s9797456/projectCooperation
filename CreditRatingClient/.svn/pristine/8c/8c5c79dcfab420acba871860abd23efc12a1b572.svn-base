.pointer-animate {
    -webkit-animation: pointer-animate .4s both ease-in;
    -moz-animation: pointer-animate .4s both ease-in;
    animation: pointer-animate .4s both ease-in
}

@-webkit-keyframes pointer-animate {
    0% {
        -webkit-transform: scale(0.2);
        opacity: .2
    }

    100% {
        -webkit-transform: scale(2);
        opacity: 0
    }
}

@-moz-keyframes pointer-animate {
    0% {
        -moz-transform: scale(0.2);
        opacity: .2
    }

    100% {
        -moz-transform: scale(2);
        opacity: 0
    }
}

@keyframes pointer-animate {
    0% {
        transform: scale(0.2);
        opacity: .2
    }

    100% {
        transform: scale(2);
        opacity: 0
    }
}

